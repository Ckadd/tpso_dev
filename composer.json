{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arubacao/php-cs-fixer-config": "^0.2.0",
        "barryvdh/laravel-cors": "^0.11.2",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "caffeinated/themes": "^4.1",
        "chencha/share": "^5.2",
        "fideloper/proxy": "^4.0",
        "ixudra/curl": "^6.16",
        "larapack/voyager-hooks": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelium/feed": "^3.0",
        "laravelium/sitemap": "^3.0",
        "maatwebsite/excel": "^3.1",
        "solarium/solarium": "^4.2",
        "soundasleep/html2text": "^0.5.0",
        "spatie/laravel-sitemap": "^5.2",
        "tcg/voyager": "1.1.11"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "phpmailer/phpmailer": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "cs": "vendor/bin/php-cs-fixer fix app/ config/ resources/lang --config=.php_cs  -vv"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
